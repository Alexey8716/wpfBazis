<Page x:Class="wpfMList.UserPage_ListOne"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:my="clr-namespace:wpfMList"
      xmlns:my1="clr-namespace:wpfGeneral.UserControls;assembly=wpfGeneral"
      x:Name="UserPage"
      mc:Ignorable="d" FontSize="14" Loaded="Page_Loaded">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Label Grid.Row="0">Тут будут фильтры</Label>
        <!--Главный список протоколов-->
        <!--Главная таблица Назначений-->
        <DataGrid ItemsSource="{Binding}" 
                  Name="PART_DataGrid" 
                  Grid.Row="1" 
                  FrozenColumnCount="1" 
                  AutoGenerateColumns="False" 
                  MinRowHeight="30"
                  CellEditEnding="PART_DataGrid_CellEditEnding"
                 >
        </DataGrid>
        
    </Grid>
</Page>
